{
  "name": "E-moji Mail",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Haking Mangos",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
  	{
  		"matches": [ "*://mail.google.com/*" ],
  		"js":[
  			"jquery-1.10.2.min.js",
  			"gmail.js",
  			"popup.js"
  		],
  		"run_at": "document_end"
  	}
  ],

  "permissions" : [
  	"https://*/*"
  	]
}